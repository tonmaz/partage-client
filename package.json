    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.5.3",
    "html-react-parser": "^1.2.8",
    "madge": "5.0.1",
    "moment": "2.29.1",
    "next": "11.1.0",
    "next-translate": "1.0.7",
    "node-plop": "0.26.2",
    "nprogress": "0.2.0",
