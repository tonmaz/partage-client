    "@popperjs/core": "2.9.3",
    "@react-page/editor": "^3.2.1",
    "@tiptap/core": "^2.0.0-beta.99",
    "@tiptap/extension-paragraph": "^2.0.0-beta.17",
    "@tiptap/extension-placeholder": "^2.0.0-beta.27",
